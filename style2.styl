/* Navigation Section */

html {
    box-sizing: border-box;
    -webkit-font-smoothing: antialiased; /* keeps smooth tye */
    height: 100%;
    width: 100%;

}

*, *:before, *:after {
    box-sizing: inherit;
}

body {
    background: #eeeeee;
    font-family: 'Roboto', sans-serif; 
    font-size: 24px; 
    color: #838994; 
    letter-spacing: 0.03em;
    height: 100%;
    width: 100%;

}

.content {
    padding: 0 0.938rem;
    margin: 0 auto;
    min-width: 37.5rem;
}

:focus {
    outline: none;
}


/* Navigation Section */

.top-navigation {
    position: fixed;
    width: 100%;
    z-index: 9999;
    background: #fff;
    border-bottom: 1px solid #DDD;
}

.top-navigation > .content {
    margin: 0 auto; /* centering */
    max-width: 1200px;
    height: 4.375rem;
}

.top-navigation .logo {
    /* margin-left: 8em; */
    /*height: 72px; */
    height: 4.5rem;
    /* padding: 0 1em; */
    z-index: 9999; /* puts on top */
}

.top-navigation .links {
    float: right;
    font-family: 'Roboto', sans-serif;
    color: #838994;
    font-size: 1.04rem;
    margin-top: 1.8rem;
    -webkit-font-smoothing: antialiased;
}
.top-navigation .links li {
    display: inline-block;
    /*margin-left: 25px; or */
    margin-left: 1.563rem;
}
.top-navigation .links .active {
    color: #4c5058;
    font-weight: 500;
}
.top-navigation .links li:hover {
    cursor: pointer;
    color: #333333;
}

/* note The specificity of the navigation menu*/

.hero {
    height: 100%;
    width: 100%;
}

.hero {
    background-image: url('../images/background.jpg');
    background-size: cover; 
    background-repeat: no-repeat;
    /* height: 50em; */
    /* Layout Declarations */
    text-align: center;
    background-position: center center;
    height: 45.938rem;
    overflow: auto; /* ?' good explanation*/
}

@media all and (max-width: 699px) {
  hero {
    background-color: #000; 
    background-image: url('../images/background.jpg');
    background-size: 200px 200px;
    background-repeat: no-repeat;
    background-position: center center;
    }
}

.hero > .content {
    margin: 0 auto; /* centering */
    max-width: 1200px;
    margin-top: 8em;
}

.hero h1, 
.hero h2 {
    color: white;
    text-align: center;
    text-transform: uppercase;
    font-weight: 300;
     /* Keeping below repetitive CSS to remind self to bring together alike*/
}


.hero h1 {
    font-style: italic;
    font-size: 4em;
    line-height: 1em;
    text-shadow: 1px 1px 0px black;
}

.hero h2 {
    font-size: 1em;
    text-shadow: 1px 1px 0px black;
    margin: 0 0 50px 0;
    line-height: 1.8em;

}
